
formulas(assumptions).

% Ternary Boolean Algebra.  Independence of axiom 2.

% Mace4 should produce a counterexample immediately.

  f(f(v,w,x),y,f(v,w,z)) = f(v,w,f(x,y,z)) # label(TBA_1).
% f(y,x,x) = x    # label(TBA_2).  % We're showing independence of this axiom.
  f(x,x,y) = x    # label(TBA_3).
  f(g(y),y,x) = x # label(TBA_4).
  f(x,y,g(y)) = x # label(TBA_5).

end_of_list.

formulas(goals).

  f(y,x,x) = x # label(TBA_2).

end_of_list.
